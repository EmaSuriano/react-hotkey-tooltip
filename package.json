{
  "name": "react-hotkey-tooltip",
  "version": "4.0.0",
  "description": "React component that creates hotkeys for your components and shows a tooltip of it",
  "main": "lib/index.js",
  "module": "es/index.js",
  "files": [
    "css",
    "es",
    "lib",
    "umd"
  ],
  "scripts": {
    "build-old": "nwb build-react-component",
    "build": "rm -rf es lib && tsc tsc -p tsconfig.cjs.json && tsc -p tsconfig.es.json",
    "build:watch": "tsc -p tsconfig.es.json --watch",
    "docz:dev": "docz dev",
    "docz:build": "docz build",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "lint": "eslint ./src --ext tsx,ts"
  },
  "dependencies": {
    "@tippyjs/react": "^4.0.0-alpha.4",
    "mousetrap": "^1.6.1"
  },
  "peerDependencies": {
    "react": "^16.3.0"
  },
  "devDependencies": {
    "@types/mousetrap": "^1.6.3",
    "@types/node": "^13.9.3",
    "@types/react": "^16.9.25",
    "babel-eslint": "^10.0.0",
    "babel-jest": "^22.4.3",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "docz": "latest",
    "docz-core": "^2.2.0",
    "enzyme": "^3.6.0",
    "enzyme-adapter-react-16": "^1.5.0",
    "eslint": "^6.0.0",
    "eslint-config-airbnb": "^18.0.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.14.2",
    "jest": "^23.4.1",
    "raf": "^3.4.0",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-github-corner": "^2.1.0",
    "typescript": "^3.8.3"
  },
  "author": "Ema Suriano",
  "jest": {
    "setupFiles": [
      "raf/polyfill",
      "<rootDir>/test-setup"
    ],
    "transform": {
      "^.+\\.js$": "<rootDir>/jest.transform.js"
    },
    "moduleDirectories": [
      "node_modules"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!**/setupTests.js",
      "!src/index.js"
    ]
  },
  "homepage": "https://react-hotkey-tooltip.netlify.com/",
  "license": "MIT",
  "repository": "https://github.com/EmaSuriano/react-hotkey-tooltip",
  "keywords": [
    "react-component",
    "hotkey",
    "tooltip",
    "shortcut"
  ]
}
