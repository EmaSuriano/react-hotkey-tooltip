{
  "name": "react-hotkey-tooltip",
  "version": "0.0.0",
  "description": "React component that creates hotkeys for your components and shows a tooltip of it",
  "main": "lib/index.js",
  "module": "es/index.js",
  "files": [
    "es",
    "lib"
  ],
  "scripts": {
    "build:esm": "rm -rf es && tsc -p tsconfig.es.json",
    "build:lib": "rm -rf lib && tsc",
    "build": "yarn build:lib & yarn build:esm",
    "check-types": "tsc --noEmit",
    "test": "jest",
    "lint": "eslint ./src --ext tsx,ts",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "release": "npx semantic-release"
  },
  "dependencies": {
    "@tippyjs/react": "^4.0.0-alpha.4",
    "jest": "^27.1.0",
    "mousetrap": "^1.6.5",
    "mousetrap-global-bind": "^1.1.0"
  },
  "peerDependencies": {
    "react": "^16"
  },
  "devDependencies": {
    "@babel/core": "^7.15.0",
    "@babel/preset-env": "^7.15.0",
    "@babel/preset-react": "^7.12.13",
    "@babel/preset-typescript": "^7.15.0",
    "@material-ui/core": "^4.12.3",
    "@storybook/addon-info": "^5.3.18",
    "@storybook/react": "^6.3.7",
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/react": "^12.0.0",
    "@types/jest": "^27.0.1",
    "@types/mousetrap": "^1.6.8",
    "@types/node": "^16.7.10",
    "@types/react": "^17.0.19",
    "@typescript-eslint/eslint-plugin": "^4.30.0",
    "@typescript-eslint/parser": "^4.30.0",
    "babel-jest": "^27.1.0",
    "babel-loader": "^8.1.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-app": "^10.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.25.1",
    "prettier": "^2.0.1",
    "prettier-eslint": "^13.0.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "typeface-roboto": "^1.1.13",
    "typescript": "^4.4.2"
  },
  "author": "Ema Suriano",
  "homepage": "https://emasuriano.github.io/react-hotkey-tooltip",
  "license": "MIT",
  "repository": "https://github.com/EmaSuriano/react-hotkey-tooltip",
  "keywords": [
    "react-component",
    "hotkey",
    "tooltip",
    "shortcut"
  ],
  "jest": {
    "setupFilesAfterEnv": [
      "<rootDir>/jest-setup.js"
    ]
  }
}
