{
  "name": "react-hotkey-tooltip",
  "version": "0.0.0-development",
  "description":
    "React component that creates hotkeys for your components and shows a tooltip of it",
  "main": "lib/index.js",
  "module": "es/index.js",
  "files": ["css", "es", "lib", "umd"],
  "scripts": {
    "build": "nwb build-react-component",
    "clean": "nwb clean-module && nwb clean-demo",
    "start": "nwb serve-react-demo",
    "test": "jest  --coverage",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "deploy-gh": "gh-pages -d demo/dist",
    "travis-deploy-once": "travis-deploy-once",
    "commit": "git-cz",
    "semantic-release": "semantic-release",
    "debug": "node --debug-brk --inspect ./node_modules/.bin/jest -i"
  },
  "dependencies": {
    "mousetrap": "^1.6.1",
    "raf": "^3.4.0",
    "react-proptypes": "^1.0.0",
    "react-tippy": "^1.2.2"
  },
  "peerDependencies": {
    "react": "16.x"
  },
  "devDependencies": {
    "babel-jest": "^22.4.3",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "commitizen": "^2.9.6",
    "cz-conventional-changelog": "^2.1.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "fd-slicer": "^1.0.1",
    "gh-pages": "^1.1.0",
    "jest": "^22.4.3",
    "nwb": "0.21.x",
    "nwb-sass": "^0.8.2",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-github-corner": "^2.1.0",
    "react-syntax-highlighter": "^7.0.0",
    "react-test-renderer": "^16.2.0",
    "semantic-release": "^15.1.4",
    "styled-components": "^3.1.6",
    "travis-deploy-once": "^5.0.0",
    "webfontloader": "^1.6.28"
  },
  "author": {
    "name": "Emanuel Suriano",
    "email": "emanuel.suriano@gmail.com",
    "url": "http://emasuriano.github.com/"
  },
  "jest": {
    "setupFiles": ["raf/polyfill", "<rootDir>/test-setup.js"],
    "transform": {
      "^.+\\.js$": "<rootDir>/jest.transform.js"
    },
    "moduleDirectories": ["node_modules"],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!**/setupTests.js",
      "!src/index.js"
    ]
  },
  "homepage": "https://emasuriano.github.io/react-hotkey-tooltip",
  "license": "MIT",
  "repository": "https://github.com/EmaSuriano/react-hotkey-tooltip",
  "keywords": ["react-component", "hotkey", "tooltip", "shortcut"],
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
