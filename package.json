{
  "name": "react-hotkey-tooltip",
  "version": "0.0.0-development",
  "description": "React component that creates hotkeys for your components and shows a tooltip of it",
  "main": "lib/index.js",
  "module": "es/index.js",
  "files": [
    "css",
    "es",
    "lib",
    "umd"
  ],
  "scripts": {
    "build": "nwb build-react-component",
    "clean": "nwb clean-module && nwb clean-demo",
    "start": "nwb serve-react-demo",
    "test": "nwb test-react",
    "test:coverage": "nwb test-react --coverage",
    "test:watch": "nwb test-react --server",
    "deploy-gh": "gh-pages -d demo/dist",
    "travis-deploy-once": "travis-deploy-once",
    "commit": "git-cz",
    "prepublish": "npm run build",
    "semantic-release":
      "semantic-release pre && npm publish && semantic-release pos"
  },
  "dependencies": {
    "mousetrap": "^1.6.1",
    "react-tippy": "^1.2.2",
    "react-proptypes": "^1.0.0",
    "react-dom": "^16.2.0"    
  },
  "peerDependencies": {
    "react": "16.x"
  },
  "devDependencies": {
    "commitizen": "^2.9.6",
    "cz-conventional-changelog": "^2.1.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "fd-slicer": "^1.0.1",
    "gh-pages": "^1.1.0",
    "nwb": "0.21.x",
    "react": "^16.2.0",
    "semantic-release": "^12.4.1",
    "styled-components": "^3.1.2",
    "travis-deploy-once": "^4.3.4"
  },
  "author": {
    "name": "Emanuel Suriano",
    "email": "emanuel.suriano@gmail.com",
    "url": "http://emasuriano.github.com/"
  },
  "homepage": "https://emasuriano.github.io/react-hotkey-tooltip",
  "license": "MIT",
  "repository": "https://github.com/EmaSuriano/react-hotkey-tooltip",
  "keywords": [
    "react-component",
    "hotkey",
    "tooltip",
    "shortcut"
  ],
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
